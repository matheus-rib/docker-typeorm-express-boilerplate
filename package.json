{
  "name": "docker-typeorm-express-boilerplate",
  "version": "1.0.0",
  "main": "/src/server.ts",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch 'src/' --exec \"ts-node src/server.ts\" -e ts",
    "test": "jest --maxWorkers=1",
    "typeorm-ts": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "docker:test": "docker-compose -p dev-crud-test -f docker-compose-test.yml run --rm test",
    "docker:migration:run": "docker-compose -f docker-compose-cli.yml run --rm cli yarn typeorm-ts migration:run -- -d ./src/config/ormConfig.ts",
    "docker:migration:generate": "ts-node ./helpers/migrationGenerate.ts"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.0",
    "@faker-js/faker": "^8.1.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.2",
    "@types/response-time": "^2.3.6",
    "@types/supertest": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typedi": "^0.10.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http": "^0.0.1-security",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "typeorm": "^0.3.17"
  }
}
